{
  "name": "Interhyp",
  "version": "1.0",
  "description": "Interhyp Home for Chrome",
  "manifest_version": 2,
  "permissions": [
      "https://www.interhyp.de/"
  ],
  "background": {
    "scripts": ["zins_crawler.js"],
    "persistent": false
  },
  "content_scripts": [{
      "css": ["immoscout.css"],
      "js": ["scrap_immo_data.js", "immoscout_show_zinscheck.js"],
      "matches": ["https://www.immobilienscout24.de/expose/*"]
    }]
}
